<div class="landing-page">
  <section class="lp-masthead">
    <div class="container">
      <div class="row">
        <div class="cta flex-view">
            {{ content.masthead }}
        </div>
      </div>
    </div>
  </section>
  <section>
    <div class="container">
      <div class="row">
          {{ content.information }}
      </div>
    </div>
  </section>
  <section class="lp-policies">
    <div class="container">
      <div class="row">
          {{ content.policies }}
      </div>
    </div>
  </section>
  {% if content.resources  %}
  <section>
    <div class="container">
      <div class="row">
          {{ content.resources }}
      </div>
    </div>
  </section>
  {% endif %}
  {% if content.blog %}
  <section>
    <div class="container">
      <div class="row">
          {{ content.blog }}
      </div>
    </div>
  </section>
  {% endif %}
  {% if content.events %}
  <section class="lp-events">
    <div class="container">
      <div class="row">
          {{ content.events }}
      </div>
    </div>
  </section>
  {% endif %}
</div>